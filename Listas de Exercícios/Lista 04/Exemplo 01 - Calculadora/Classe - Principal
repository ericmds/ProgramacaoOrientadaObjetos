package ExemploCalculadora;

public class Principal {

	public static void main(String[] args) {
		Calculadora calc = new Calculadora();
		calc.n1 = 7.14;
		calc.n2 = 8.23;
		
		double retorno;
		retorno = calc.somar();
		
		// MÉTODO 1
		System.out.println("Resultado da soma: "+retorno);
		// MÉTODO 2
		System.out.println("Resultado da soma: "+calc.somar());
		// MÉTODO 3
		calc.multiplicar();
		// MÉTODO 4
		calc.dividir(4.17, 5.43);
		// MÉTODO 5
		retorno = calc.subtrair(5.44, 2.17);
		System.out.println("Resultado da subtração: "+retorno);
	}

}
